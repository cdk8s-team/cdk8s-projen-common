      - name: Setup Node.js
        uses: actions/setup-node@v3
        with:
          node-version: 16.20.0
      - name: Install dependencies
        run: yarn install --check-files --frozen-lockfile
      - name: release
        run: npx projen release
      - name: Check for new commits
